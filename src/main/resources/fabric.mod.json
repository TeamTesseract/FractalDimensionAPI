{
  "schemaVersion": 1,
  "id": "fractal",
  "version": "${version}",

  "name": "Fractal Dimension API",
  "description": "A lightweight dimensional API for disposable, low-overhead pocket dimensions.",
  "authors": [ "Vatuu" ],

  "license": "MIT",
  "icon": "assets/fractal/icon.png",
  "custom": {
    "modmenu:api": true
  },

  "accessWidener": "fractal.widener",

  "entrypoints": {
    "main": [
      "dev.teamtesseract.fractal.Fractal"
    ],
    "client": [
      "dev.teamtesseract.fractal.client.FractalClient"
    ]
  },
  "mixins": [
    "mixins.fractal.json",
    "mixins.client.fractal.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric-events-lifecycle-v0": ">=0.1.1"
  }
}
